{
  "name": "hobnoob",
  "version": "0.1.0",
  "description": "A small user directory",
  "main": "index.js",
  "scripts": {
    "test": "yarn run test:unit && yarn test:integration && yarn run test:e2e",
    "build": "rm -rf dist/ && babel src -d dist --copy-files",
    "serve": "yarn run build && dotenv -e envs/.env node dist",
    "watch": "nodemon -w src --exec yarn run serve",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "precommit": "yarn run lint",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env ./scripts/e2e.test.sh",
    "test:unit": "dotenv -e env/test.env -e env/.env mocha -- src/**/*.unit.test.js --require @babel/register",
    "test:integration": "dotenv -e envs/test.env -e envs/.env mocha -- src/**/*.integration.test.js --require @babel/register",
    "test:unit:coverage": "nyc --reporter=html --reporter=text yarn run test:unit",
    "test:e2e:coverage": "nyc --reporter=html --reporter=text yarn run test:e2e",
    "test:coverage": "nyc --reporter=html --reporter=text yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yanmifeakeju/hobnob.git"
  },
  "author": "Oluwayanmife Akeju <yanmifeakeju@gmail.com> (https://yanmifeakeju.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanmifeakeju/hobnob/issues"
  },
  "homepage": "https://github.com/yanmifeakeju/hobnob#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@cucumber/cucumber": "^8.0.0-rc.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^9.1.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "superagent": "^6.1.0"
  },
  "volta": {
    "node": "16.13.1"
  },
  "dependencies": {
    "ajv": "^8.9.0",
    "dotenv-cli": "^4.1.1",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.2"
  }
}
